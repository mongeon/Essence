<MudTable Items="@Entries" >
    <HeaderContent>

        <MudTh>Supplier</MudTh>
        <MudTh>Date</MudTh>
        <MudTh>Kilometers</MudTh>
        <MudTh>Total Price</MudTh>
        <MudTh>Liters</MudTh>
        <MudTh>L/100kms</MudTh>
        <MudTh>$/L</MudTh>

    </HeaderContent>
    <RowTemplate>
        <MudTd>@context.Supplier.Name</MudTd>
        <MudTd>@context.Date.ToShortDateString()</MudTd>
        <MudTd>@context.Kilometers.ToString("N0")</MudTd>
        <MudTd>@context.TotalPrice.ToString("C2")</MudTd>
        <MudTd>@context.Liters.ToString("N3")</MudTd>
        <MudTd>@context.Consumption.ToString("N3")</MudTd>
        <MudTd>@context.LiterPrice.ToString("C3")</MudTd>
    </RowTemplate>
</MudTable>

@code {
    [Parameter]
    public IEnumerable<Entry> Entries { get; set; } = new List<Entry>();
}
